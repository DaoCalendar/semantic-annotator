{
  "rules": [
    {
      "pattern": "(au|à (@DET)?)",
      "substitutions": "1:AT",
      "samples": [ "à", "au" ]
    },
    {
      "pattern": "(@AT moins)",
      "substitutions": "1:ATLEAST",
      "samples": [ "si au moins" ]
    },
    {
      "pattern": "(@AT limite)",
      "substitutions": "1:ATLEAST",
      "samples": [ "si à la limite" ]
    }
  ]
}